{
	"name": "studio210",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"deploy": "bun run build && wrangler pages deploy .svelte-kit/cloudflare",
		"deploy:production": "bun run build && wrangler pages deploy .svelte-kit/cloudflare --branch=main",
		"pages:dev": "wrangler pages dev .svelte-kit/cloudflare --compatibility-date=2026-01-22"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.2",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-cloudflare": "^7.2.5",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.50.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@types/leaflet": "^1.9.21",
		"@types/node": "^24.10.9",
		"@vitest/browser-playwright": "^4.0.18",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^16.5.0",
		"playwright": "^1.57.0",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.4.1",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.48.0",
		"svelte-check": "^4.3.5",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.1",
		"vite": "^7.3.1",
		"vitest": "^4.0.18",
		"vitest-browser-svelte": "^2.0.1",
		"wrangler": "^4.60.0"
	},
	"dependencies": {
		"@emailjs/browser": "^4.4.1",
		"date-fns": "^4.1.0",
		"leaflet": "^1.9.4",
		"zod": "^4.3.5"
	}
}
